{
  "name" : "Leo Duboku",
  "version" : "1.2",
  "description" : "Helps debug Polymer apps.",

  "devtools_page": "devtools.html",
  "permissions": ["webNavigation", "activeTab", "tabs"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["perfContentScript.js"],
      "run_at": "document_start"
    }
  ]
}
